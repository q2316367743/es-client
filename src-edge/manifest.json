{
    "manifest_version": 3,
    "name": "es-client",
    "version": "3.1.5",
    "description": "elasticsearch管理客户端",
    "content_security_policy": {
        "script-src": "self",
        "object-src": "self"
    },
    "icons": {
        "96": "icons/logo_200.png",
        "128": "icons/logo_200.png"
    },
    "action": {
        "default_title": "es-client",
        "default_icon": "icons/logo_200.png"
    },
    "background": {
        "service_worker": "src/background.js",
        "type": "module"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "*://*/"
    ]
}
